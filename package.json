{
  "name": "node-typescript-starter",
  "version": "1.0.0",
  "description": "Node Typescript Boilerplate",
  "main": "server.js",
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./build",
    "dev": "nodemon",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "set NODE_ENV=production&& node ./build/server",
    "test": "jest --detectOpenHandles",
    "test: watch": "jest --watch --detectOpenHandles",
    "typecheck": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/B-Jones-RFD/node-typescript-starter.git"
  },
  "author": "Author",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/B-Jones-RFD/node-typescript-starter/issues"
  },
  "homepage": "https://github.com/B-Jones-RFD/node-typescript-starter#readme",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "express-winston": "^4.2.0",
    "http-errors": "^2.0.0",
    "swagger-ui-express": "^4.6.0",
    "tiny-invariant": "^1.3.1",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/express-status-monitor": "^1.3.0",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.9.4"
  }
}